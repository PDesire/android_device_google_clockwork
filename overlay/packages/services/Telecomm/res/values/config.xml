<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Package name for the default in-call UI and dialer [DO NOT TRANSLATE] -->
    <string name="ui_default_package" translatable="false">com.google.android.apps.wearable.phone</string>

    <!-- Class name for the default main Dialer activity [DO NOT TRANSLATE] -->
    <string name="dialer_default_class" translatable="false">com.google.android.clockwork.phone.dialer.DialerActivity</string>

    <!-- Class name for the default in-call UI Service [DO NOT TRANSLATE] -->
    <string name="incall_default_class" translatable="false">com.google.android.clockwork.phone.incall.InCallServiceImpl</string>

    <!-- Prevents audio from being routed to the speaker when docked -->
    <bool name="use_speaker_when_docked">false</bool>

    <!-- Flag indicating whether allow (silence rather than reject) the incoming call if it has a
         different source (connection service) from the existing ringing call when reaching
         maximum ringing calls. -->
    <bool name="silence_incoming_when_different_service_and_maximum_ringing">true</bool>
</resources>
