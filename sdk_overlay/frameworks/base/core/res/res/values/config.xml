<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- This device allows sms service. -->
    <bool name="config_sms_capable">true</bool>

    <!-- Enable mobile network which provides connectivity for emulator devices -->
    <string-array translatable="false" name="networkAttributes">
        <item>"mobile,0,0,0,-1,true"</item>
        <item>"wifi,1,1,1,-1,true"</item>
        <item>"bluetooth,7,7,2,-1,true"</item>
        <item>"proxy,16,16,1,-1,true"</item>
    </string-array>

    <!-- Enable display overlay for circular emulators -->
    <bool name="config_windowEnableCircularEmulatorDisplayOverlay">true</bool>

    <!-- Overide the power button behavior on emulator to simply sleep
         and not also go home like devices do, since there is no other way
         to enter ambient mode for the current activity. Revert this as
         part of http://b/20051148. -->
    <integer name="config_shortPressOnPowerBehavior">1</integer>
</resources>
